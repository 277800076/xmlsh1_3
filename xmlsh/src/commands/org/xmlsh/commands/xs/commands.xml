<commands>
 
   <function name="string" url="http://www.xmlsh.org/FunctionString">
		    <synopsis>Convert to string</synopsis>
		    <usage>
			      <para>string( args )</para>
		    </usage>
	  </function>
   
   <function name="integer" url="http://www.xmlsh.org/FunctionInteger">
		    <synopsis>Convert to integer</synopsis>
		    <usage>
			      <para>integer( arg )</para>
		    </usage>
	  </function>
	  
	     <function name="QName" url="http://www.xmlsh.org/FunctionQName">
		    <synopsis>Convert to QName</synopsis>
		    <usage>
			      <para>QName( qname )</para>
			      <para>QName( uri lexical )</para>
			      <para>QName( prefix uri lexical)</para>

		    </usage>
	  </function>
	  
	  
</commands>
