<patterns xmlns="http://www.xmlsh.org/jxon">
	<pattern name="full">
		
		<children wrap="true" name="_children"/>
		<attributes wrap="true" name="_attributes"/>
		<text wrap="false"/>
		<value type="schema"/>

	</pattern>



	<pattern name="simple">
		<children wrap="false" />
		<attributes wrap="false"/>
		<text wrap="true" name="_text"/>
		<value type="schema"/>

	</pattern>

</patterns>